<h1> <%= title %> </h1>
<form action='/register' method='post'>
  <% ['email','password','first_name','last_name'].each do |field| %>
    <input name='<%= field %>' type=<%= field %> placeholder='<%= field %>' value='<%=(cu || {})[field]%>'>
  <% end %>
  
  <button type='submit'> Submit </button>
</form>
